{
  "name": "react-extended-boilerplate",
  "version": "0.0.1",
  "description": "Template project for React",
  "scripts": {
    "analyze": "yarn clean && cross-env NODE_ENV=production webpack  --config webpack/analyze.config.js --progress --color",
    "clean": "rimraf ./dist",
    "build": "yarn run clean && cross-env NODE_ENV=production webpack --mode production --config webpack/prod.config.js --progress --color && cp -a docker/* dist/docker",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --config webpack/dev.config.js --progress --color",
    "prod": "cross-env NODE_ENV=development webpack-dev-server --mode development --config webpack/prod.config.js --progress --color",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "sonar": "sonar-scanner",
    "test": "jest --config=jest.json --no-cache",
    "test:coverage": "rm -rf coverage/ && jest --config=jest.json --noStackTrace --all --coverage --silent --no-cache"
  },
  "jestSonar": {
    "reportPath": "dist/report",
    "reportFile": "TESTS-results.xml"
  },
  "author": "Mikhail Prokofev",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/enzyme": "^3.1.6",
    "@types/jest": "^23.3.2",
    "@types/query-string": "^6.1.1",
    "@types/react": "^16.0.31",
    "@types/react-addons-test-utils": "^0.14.20",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-dom": "^16.0.11",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-test-renderer": "^16.0.0",
    "@types/redux-promise": "^0.5.28",
    "@types/webpack-env": "^1.13.3",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-preset-jest": "^23.2.0",
    "babel-preset-stage-3": "^6.24.1",
