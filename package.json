    "jest-sonar-reporter": "2.0.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.7.2",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.0",
    "postcss-scss": "^2.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "redux-devtools": "^3.4.1",
